type File implements Node {
  contentType: String!
  createdAt: String!
  id: ID! @isUnique
  name: String!
  secret: String! @isUnique
  size: Int!
  updatedAt: String!
  url: String! @isUnique
}

type User implements Node {
  createdAt: String!
  id: ID! @isUnique
  email: String @isUnique
  updatedAt: String!
  name: String!
  password: String
}

type Call implements Node {
  started: String!
  finished: String!
  recipient: Contact! @relation(name: "RecipientCalls")
  caller: Contact! @relation(name: "CallerCalls")
  createdAt: String!
  id: ID! @isUnique
  updatedAt: String!
}

type Contact implements Node {
  id: ID! @isUnique
  name: String!
  age: Int!
  phone: String!
  country: String!
  city: String!
  street: String!
  apt: String!
  recievedCalls: [Call!]! @relation(name: "RecipientCalls")
  executedCalls: [Call!]! @relation(name: "CallerCalls")
  createdAt: String!
  updatedAt: String!
}
